<odoo>
  <data noupdate="1">
    <record id="ir_cron_post_call_payment_transaction" model="ir.cron">
        <field name="name">Post Call to API for Payment Transactions</field>
        <field name="model_id" ref="payment.model_payment_transaction"/>
        <field name="state">code</field>
        <field name="code">model.cron_send_payment_transaction_post_call()</field>
        <field name="interval_number">3</field>
        <field name="interval_type">minutes</field>
        <field name="nextcall" eval="(DateTime.utcnow()).strftime('%Y-%m-%d %H:%M:%S')"/>
        <field name="active" eval="True"/>
    </record>
  </data>
</odoo>
